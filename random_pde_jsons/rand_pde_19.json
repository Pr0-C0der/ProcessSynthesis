              {
                "type": "param",
                "name": "alpha"
              },
              {
                "type": "op",
                "op": "*",
                "args": [
                  {
                    "type": "deriv",
                    "dep": "u",
                    "wrt": "x",
                    "order": 2
                  },
                  {
                    "type": "deriv",
                    "dep": "u",
                    "wrt": "x",
                    "order": 2
                  }
                ]
              }
            ]
          },
          {
            "type": "op",
            "op": "*",
            "args": [
              {
