                "type": "param",
                "name": "b"
              },
              {
                "type": "op",
                "op": "*",
                "args": [
                  {
                    "type": "dep",
                    "name": "u"
                  },
                  {
                    "type": "deriv",
                    "dep": "u",
                    "wrt": "x",
                    "order": 1
                  }
                ]
              }
            ]
          }
        ]
      },
      "rhs": {
        "type": "op",
        "op": "+",
        "args": [
          {
            "type": "op",
            "op": "*",
            "args": [
              {
                "type": "param",
                "name": "c"
              },
              {
                "type": "deriv",
                "dep": "u",
                "wrt": "x",
                "order": 2
              }
            ]
          },
          {
            "type": "op",
            "op": "*",
            "args": [
              {
                "type": "param",
                "name": "d"
              },
              {
                "type": "op",
                "op": "*",
                "args": [
                  {
                    "type": "deriv",
                    "dep": "u",
                    "wrt": "x",
                    "order": 2
                  },
                  {
                    "type": "deriv",
                    "dep": "u",
                    "wrt": "x",
                    "order": 2
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ],
  "initial_conditions": [
