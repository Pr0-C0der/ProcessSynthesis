      "equation_id": "rand_pde_14",
      "type": "equals",
      "lhs": {
        "type": "op",
        "op": "+",
        "args": [
          {
            "type": "deriv",
            "dep": "u",
            "wrt": "t",
            "order": 1
          },
          {
            "type": "op",
            "op": "*",
            "args": [
