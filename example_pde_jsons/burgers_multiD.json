      "equation_id": "u_evolution",
      "type": "equals",
      "lhs": {
        "type": "op",
        "op": "+",
        "args": [
          {
            "type": "deriv",
            "dep": "u",
            "wrt": "t",
            "order": 1
          },
          {
            "type": "op",
            "op": "+",
            "args": [
              {
                "type": "op",
                "op": "*",
                "args": [
                  {
                    "type": "dep",
                    "name": "u"
                  },
                  {
                    "type": "deriv",
                    "dep": "u",
                    "wrt": "x",
                    "order": 1
                  }
                ]
              },
              {
                "type": "op",
                "op": "*",
                "args": [
                  {
                    "type": "dep",
                    "name": "v"
                  },
                  {
                    "type": "deriv",
                    "dep": "u",
                    "wrt": "y",
                    "order": 1
                  }
                ]
              }
            ]
          }
        ]
      },
      "rhs": {
        "type": "op",
        "op": "*",
        "args": [
          {
            "type": "param",
            "name": "nu"
          },
          {
            "type": "op",
            "op": "+",
            "args": [
              {
                "type": "deriv",
                "dep": "u",
                "wrt": "x",
                "order": 2
              },
              {
                "type": "deriv",
                "dep": "u",
                "wrt": "y",
                "order": 2
