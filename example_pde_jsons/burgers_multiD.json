        "op": "+",
        "args": [
          {
            "type": "deriv",
            "dep": "v",
            "wrt": "t",
            "order": 1
          },
          {
            "type": "op",
            "op": "+",
            "args": [
              {
                "type": "op",
                "op": "*",
                "args": [
                  {
                    "type": "dep",
                    "name": "u"
                  },
                  {
                    "type": "deriv",
                    "dep": "v",
                    "wrt": "x",
                    "order": 1
                  }
                ]
              },
              {
                "type": "op",
                "op": "*",
                "args": [
                  {
                    "type": "dep",
                    "name": "v"
                  },
                  {
                    "type": "deriv",
                    "dep": "v",
                    "wrt": "y",
                    "order": 1
                  }
                ]
              }
