{
  "metadata": {
    "name": "2D Burgers system (u,v) - multi-D without pressure",
    "description": "PDE system: u_t + u u_x + v u_y = nu (u_xx + u_yy), v_t + u v_x + v v_y = nu (v_xx + v_yy)"
  },
  "variables": {
    "independent": [
      "x",
      "y",
      "t"
    ],
    "dependent": [
      "u",
      "v"
    ]
  },
  "parameters": {
    "nu": 0.01
  },
  "domain": {
    "x": [
      0.0,
      1.0
    ],
    "y": [
      0.0,
      1.0
    ],
    "t": [
      0.0,
      1.0
    ]
  },
  "pdes": [
